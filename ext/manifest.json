{
	"manifest_version": 3,

	"name": "Spartan Security",
	"description": "Security monitering extension",
	"version":"1.0.0",
	"icons": {
		"16": "fav1.png"
	},

	"background": {
		"service_worker": "background.js"
	},

	"content_scripts": [{
		"matches": [
			"<all_urls>"
		],
		"js":["content.js"]
	}],

	"action": {
		"default_popup": "popup.html"
	},

	"permissions": [
		"storage",
		"tabs"
	]
}
